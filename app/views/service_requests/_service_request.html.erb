<tr>
  <td><%= service_request.full_name %></td>
  <td><%= service_request.case_number %></td>
  <td><%= service_request.created_at %></td>
  <td><%= service_request.status %></td>
  <td>
      <%= link_to "View", service_request_path(service_request), class: "btn-sm btn-warning" %> 
      <%= link_to "Edit", edit_service_request_path(service_request), class: "btn-sm btn-warning" %>
      <%= link_to "Remove", service_request_path(service_request), method: :delete, class: "btn-sm btn-warning", data: { confirm: "Are you sure?" } %>
  </td>
</tr>